<?xml version="1.0" encoding="utf-8"?>
<database name="hellofresh" defaultIdMethod="native" defaultPhpNamingMethod="underscore">
  <table name="contact" idMethod="native" phpName="Contact">
    <column name="id" phpName="Id" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
    <column name="name" phpName="Name" type="VARCHAR" size="255" required="true"/>
    <column name="surname" phpName="Surname" type="VARCHAR" size="255" required="true"/>
    <column name="email" phpName="Email" type="VARCHAR" size="255" required="true"/>
    <column name="created_at" phpName="CreatedAt" type="TIMESTAMP" required="true"/>
    <column name="updated_at" phpName="UpdatedAt" type="TIMESTAMP" required="true" defaultExpr="CURRENT_TIMESTAMP"/>
    <index name="contact_name_surname_index">
      <index-column name="name"/>
      <index-column name="surname"/>
    </index>
    <unique name="contact_email_uindex">
      <unique-column name="email"/>
    </unique>
    <vendor type="mysql">
      <parameter name="Engine" value="InnoDB"/>
    </vendor>
  </table>
</database>